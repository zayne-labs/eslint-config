{
	"name": "@zayne-labs/eslint-config",
	"version": "0.0.0",
	"type": "module",
	"description": "Zayne's ESLint config preset",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/zayne-labs/eslint-config.git"
	},
	"homepage": "https://github.com/zayne-labs/eslint-config#readme",
	"bugs": {
		"url": "https://github.com/zayne-labs/eslint-config/issues"
	},
	"license": "MIT",
	"author": "Ryan Zayne",
	"scripts": {
		"build:dev": "pnpm typegen & cross-env NODE_ENV=development tsup",
		"build:test": "pnpm typegen & concurrently --prefix-colors \"yellow.bold,#7da4f8.bold,magenta\" --names PUBLINT,TSUP,ATTW 'pnpm:lint:publint' 'pnpm:build:dev' 'pnpm:lint:attw'",
		"build": "pnpm typegen & tsup",
		"inspect:eslint-config": "pnpx @eslint/config-inspector@latest --config eslint.config.js",
		"lint:attw": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
		"lint:check-types": "tsc --pretty --incremental -p tsconfig.json",
		"lint:commitlint": "commitlint --edit",
		"lint:eslint": "eslint src/**/*.ts --cache --max-warnings 0 --report-unused-disable-directives",
		"lint:format": "prettier --cache --write .",
		"lint:packages": "pnpm dedupe --check",
		"lint:publint": "publint --strict .",
		"lint:size": "size-limit",
		"typegen": "tsx scripts/typegen.ts",
		"prepare": "husky",
		"release": "changeset publish",
		"test:release": "pkg-pr-new publish",
		"version-package": "changeset version"
	},
	"peerDependencies": {
		"@eslint-react/eslint-plugin": "^1.5.8",
		"eslint": "^9.10.0",
		"eslint-plugin-astro": "^1.2.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.4",
		"eslint-plugin-solid": "^0.14.3",
		"eslint-plugin-svelte": ">=2.35.1",
		"eslint-plugin-tailwindcss": "^3.17.4"
	},
	"peerDependenciesMeta": {
		"@eslint-react/eslint-plugin": {
			"optional": true
		},
		"eslint-plugin-astro": {
			"optional": true
		},
		"eslint-plugin-react-hooks": {
			"optional": true
		},
		"eslint-plugin-react-refresh": {
			"optional": true
		},
		"eslint-plugin-solid": {
			"optional": true
		},
		"eslint-plugin-svelte": {
			"optional": true
		},
		"eslint-plugin-tailwindcss": {
			"optional": true
		}
	},
	"dependencies": {
		"@eslint/compat": "^1.1.1",
		"@eslint/js": "^9.10.0",
		"@stylistic/eslint-plugin": "^2.8.0",
		"eslint-config-flat-gitignore": "^0.3.0",
		"eslint-import-resolver-typescript": "^3.6.3",
		"eslint-plugin-import-x": "^4.2.1",
		"eslint-plugin-jsdoc": "^50.2.3",
		"eslint-plugin-n": "^17.10.2",
		"eslint-plugin-perfectionist": "^3.6.0",
		"eslint-plugin-unicorn": "^55.0.0",
		"eslint-plugin-vue": "^9.28.0",
		"globals": "^15.9.0",
		"typescript-eslint": "^8.6.0"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.16.2",
		"@changesets/cli": "^2.27.8",
		"@commitlint/cli": "^19.5.0",
		"@commitlint/config-conventional": "^19.5.0",
		"@size-limit/esbuild-why": "^11.1.5",
		"@size-limit/preset-small-lib": "^11.1.5",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/eslint-plugin-tailwindcss": "^3.17.0",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^22.5.5",
		"@zayne-labs/tsconfig": "^0.1.1",
		"concurrently": "^9.0.1",
		"cross-env": "^7.0.3",
		"eslint-typegen": "^0.3.2",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"pkg-pr-new": "^0.0.24",
		"prettier": "^3.3.3",
		"publint": "^0.2.11",
		"size-limit": "^11.1.5",
		"tsup": "^8.2.4",
		"tsx": "^4.19.1",
		"typescript": "5.6.2"
	},
	"exports": {
		".": "./dist/index.js"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/",
		"provenance": true
	},
	"files": [
		"dist"
	],
	"size-limit": [
		{
			"path": "./src/[*]",
			"limit": "[*] kb"
		}
	],
	"sideEffects": false,
	"keywords": [],
	"engines": {
		"node": ">=18.x"
	}
}