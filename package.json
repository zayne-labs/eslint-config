{
	"name": "@zayne-labs/eslint-config",
	"type": "module",
	"version": "0.4.1",
	"description": "Zayne Labs' ESLint config preset",
	"author": "Ryan Zayne",
	"license": "MIT",
	"homepage": "https://github.com/zayne-labs/eslint-config#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/zayne-labs/eslint-config.git"
	},
	"bugs": {
		"url": "https://github.com/zayne-labs/eslint-config/issues"
	},
	"keywords": [],
	"sideEffects": false,
	"exports": {
		".": "./dist/index.js"
	},
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=18.x"
	},
	"scripts": {
		"build": "pnpm typegen && tsup",
		"build:dev": "pnpm typegen && cross-env NODE_ENV=development tsup",
		"build:test-bundle": "pnpm typegen & concurrently --prefix-colors \"yellow.bold,#7da4f8.bold,magenta\" --names PUBLINT,TSUP,ATTW 'pnpm:lint:publint' 'pnpm:build:dev' 'pnpm:lint:attw'",
		"inspect:eslint-config": "pnpx @eslint/config-inspector@latest --config eslint.config.js",
		"lint:attw": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
		"lint:check-types": "pnpm typegen && tsc --pretty -p tsconfig.json",
		"lint:commitlint": "commitlint --edit",
		"lint:eslint": "pnpm build && pnpm lint:eslint:dev",
		"lint:eslint:dev": "eslint . --max-warnings 0 --report-unused-disable-directives",
		"lint:eslint:interactive": "pnpm build && pnpm lint:eslint:interactive:dev",
		"lint:eslint:interactive:dev": "pnpx eslint-interactive@latest . --fix --max-warnings 0 --report-unused-disable-directives",
		"lint:format": "prettier --cache --write .",
		"lint:packages": "pnpm dedupe --check",
		"lint:publint": "publint --strict .",
		"prepare": "husky",
		"release": "pnpm typegen && changeset publish",
		"test:release": "pnpm typegen && pkg-pr-new publish",
		"typegen": "tsx scripts/typegen.ts",
		"version-package": "changeset version"
	},
	"peerDependencies": {
		"@eslint-react/eslint-plugin": ">=1.15.0",
		"@next/eslint-plugin-next": ">=14.2.15",
		"@tanstack/eslint-plugin-query": ">=5.59.7",
		"eslint": ">=9.12.0",
		"eslint-plugin-astro": ">=1.2.0",
		"eslint-plugin-react-hooks": ">=5.0.0",
		"eslint-plugin-react-refresh": ">=0.4.12",
		"eslint-plugin-solid": ">=0.14.3",
		"eslint-plugin-svelte": ">=2.35.1",
		"eslint-plugin-tailwindcss": ">=3.17.4",
		"eslint-plugin-vue": ">=9.28.0",
		"eslint-processor-vue-blocks": ">=0.1.2",
		"vue-eslint-parser": ">=9.4.3"
	},
	"peerDependenciesMeta": {
		"@eslint-react/eslint-plugin": {
			"optional": true
		},
		"@next/eslint-plugin-next": {
			"optional": true
		},
		"@tanstack/eslint-plugin-query": {
			"optional": true
		},
		"eslint-plugin-astro": {
			"optional": true
		},
		"eslint-plugin-react-hooks": {
			"optional": true
		},
		"eslint-plugin-react-refresh": {
			"optional": true
		},
		"eslint-plugin-solid": {
			"optional": true
		},
		"eslint-plugin-svelte": {
			"optional": true
		},
		"eslint-plugin-tailwindcss": {
			"optional": true
		},
		"eslint-plugin-vue": {
			"optional": true
		},
		"eslint-processor-vue-blocks": {
			"optional": true
		},
		"vue-eslint-parser": {
			"optional": true
		}
	},
	"dependencies": {
		"@antfu/install-pkg": "0.5.0",
		"@clack/prompts": "0.9.0",
		"@eslint-community/eslint-plugin-eslint-comments": "4.4.1",
		"@eslint/compat": "1.2.4",
		"@eslint/js": "9.17.0",
		"@stylistic/eslint-plugin": "2.12.1",
		"eslint-config-flat-gitignore": "0.3.0",
		"eslint-flat-config-utils": "0.4.0",
		"eslint-import-resolver-typescript": "3.7.0",
		"eslint-merge-processors": "^0.1.0",
		"eslint-plugin-import-x": "4.6.1",
		"eslint-plugin-jsdoc": "50.6.1",
		"eslint-plugin-jsonc": "2.18.2",
		"eslint-plugin-n": "17.15.1",
		"eslint-plugin-perfectionist": "4.4.0",
		"eslint-plugin-security": "3.0.1",
		"eslint-plugin-toml": "^0.12.0",
		"eslint-plugin-unicorn": "56.0.1",
		"eslint-plugin-yml": "^1.16.0",
		"globals": "15.14.0",
		"jsonc-eslint-parser": "2.4.0",
		"local-pkg": "0.5.1",
		"toml-eslint-parser": "0.10.0",
		"typescript-eslint": "8.18.2",
		"yaml-eslint-parser": "1.2.3"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "0.17.2",
		"@changesets/cli": "2.27.11",
		"@commitlint/cli": "19.6.1",
		"@commitlint/config-conventional": "19.6.0",
		"@eslint-react/eslint-plugin": "1.22.1",
		"@next/eslint-plugin-next": "15.1.2",
		"@size-limit/esbuild-why": "11.1.6",
		"@size-limit/preset-small-lib": "11.1.6",
		"@tanstack/eslint-plugin-query": "5.62.9",
		"@total-typescript/ts-reset": "0.6.1",
		"@types/eslint-plugin-security": "3.0.0",
		"@types/eslint-plugin-tailwindcss": "3.17.0",
		"@types/eslint__js": "8.42.3",
		"@types/node": "22.10.2",
		"@zayne-labs/eslint-config": "link:",
		"@zayne-labs/tsconfig": "0.2.1",
		"concurrently": "9.1.0",
		"cross-env": "7.0.3",
		"eslint": "9.17.0",
		"eslint-plugin-astro": "1.3.1",
		"eslint-plugin-react-hooks": "5.1.0",
		"eslint-plugin-react-refresh": "0.4.16",
		"eslint-plugin-solid": "0.14.5",
		"eslint-plugin-svelte": "2.46.1",
		"eslint-plugin-tailwindcss": "3.17.5",
		"eslint-plugin-vue": "9.32.0",
		"eslint-processor-vue-blocks": "0.1.2",
		"eslint-typegen": "0.3.2",
		"husky": "9.1.7",
		"lint-staged": "15.2.11",
		"pkg-pr-new": "0.0.39",
		"prettier": "3.4.2",
		"publint": "0.2.12",
		"size-limit": "11.1.6",
		"tailwindcss": "3.4.17",
		"tsup": "8.3.5",
		"tsx": "4.19.2",
		"typescript": "5.7.2",
		"vue-eslint-parser": "9.4.3"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/",
		"provenance": true
	}
}
